{
    "version": 2,
    "builds": [
        {
            "src": "app.ts",
            "use": "@now/node",
            "config": {
                "NODE_ENV": "production"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.ts"
        }
    ],
    "installCommand": [
        "npm install date-fns"
    ]
}